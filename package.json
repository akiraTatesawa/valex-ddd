{
  "name": "valex-ddd",
  "version": "1.0.0",
  "description": "Valex is a voucher card API. The API is responsible for creating, reloading, activating, as well as processing purchases.",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "ci:test": "jest --runInBand --coverage=false",
    "dev:load-env": "dotenv -e .env.development",
    "dev:generate": "npm run dev:load-env -- prisma generate",
    "dev:migrate": "npm run dev:load-env -- prisma migrate dev",
    "dev:prisma-studio": "npm run dev:load-env -- prisma studio",
    "dev:seed": "npm run dev:load-env -- -- ts-node -r tsconfig-paths/register ./src/infra/database/prisma/seeds/seed.ts",
    "dev": "npm run dev:load-env -- -- ts-node-dev --respawn --transpile-only --exit-child --ignore-watch node_modules -r tsconfig-paths/register ./src/infra/http/server.ts"
  },
  "prisma": {
    "schema": "./src/infra/database/prisma/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akiraTatesawa/valex-ddd.git"
  },
  "author": "Arthur Akira Tatesawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akiraTatesawa/valex-ddd/issues"
  },
  "homepage": "https://github.com/akiraTatesawa/valex-ddd#readme",
  "dependencies": {
    "@ngneat/falso": "^6.2.0",
    "@prisma/client": "^4.6.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cryptr": "^6.0.3",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.5.3",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "node": "^19.1.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.18",
    "@swc/jest": "^0.2.23",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prisma": "^4.6.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^2.0.2",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "typescript-transform-paths": "^3.4.4"
  }
}